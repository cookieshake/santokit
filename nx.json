{
  "installation": {
    "version": "22.4.2",
    "plugins": {
      "@nx/eslint": "22.4.2",
      "@nx/vitest": "22.4.2"
    }
  },
  "workspaceLayout": {
    "libsDir": "packages",
    "appsDir": "packages"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "targetDefaults": {
    "@nx/vitest:test": {
      "cache": true,
      "inputs": [
        "default",
        "^default"
      ]
    }
  }
}
